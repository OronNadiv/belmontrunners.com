{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "build": "./node_modules/.bin/tslint --project tsconfig.json && ./node_modules/.bin/tsc",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "auth2UsersCMD": "npm run build && node lib/auth2UsersCMD.js",
    "contacts2MailChimpCMD": "npm run build && node lib/contacts2MailChimpCMD.js",
    "users2ContactsCMD": "npm run build && node lib/users2ContactsCMD.js"
  },
  "main": "lib/index.js",
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "csvtojson": "^2.0.10",
    "exif-parser": "^0.1.12",
    "firebase-admin": "^8.8.0",
    "firebase-functions": "^3.3.0",
    "gravatar": "^1.8.0",
    "ical-generator": "^1.9.2",
    "image-size": "^0.8.3",
    "jimp": "^0.9.3",
    "md5": "^2.2.1",
    "md5-file": "^4.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "normalize-email": "^1.1.1",
    "parse-full-name": "^1.2.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "stripe": "^7.14.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.17",
    "firebase-functions-test": "^0.1.7",
    "json-to-ts": "^1.6.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "private": true
}
